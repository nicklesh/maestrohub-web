{
  "expo": {
    "name": "Maestro Hub",
    "slug": "maestro-hub",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/maestrohub_app_icon_only_192x192.png",
    "scheme": "maestrohub",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/maestrohub_app_icon_only_192x192.png",
        "backgroundColor": "#000"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/maestrohub_app_icon_only_192x192.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/maestrohub_app_icon_only_192x192.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#000"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
